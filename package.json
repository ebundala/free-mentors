{
  "name": "free-mentors",
  "version": "1.0.0",
  "description": "Free   Mentors   is   a   social   initiative   where   accomplished   professionals   become   role   models   to  ",
  "main": "index.js",
  "scripts": {
    "watch": "npm run clean_debug && npm run build_debug",
    "serve": "nodemon .debug/server/index.js",
    "test": "nyc --reporter=text --reporter=html mocha --recursive --require @babel/polyfill --require @babel/register",
    "build": "npm run clean && npm run build_server && npm run build_ui",
    "clean": "rm -rf dist && mkdir dist",
    "start": "nodemon dist/server/index.js",
    "lint": "./node_modules/.bin/eslint ./server/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build_test": "babel test --out-dir dist/UI --ignore node_modules --ignore test",
    "build_ui": "babel UI --out-dir dist/UI --ignore node_modules --ignore test",
    "build_server": "babel server --out-dir dist/server --ignore node_modules --ignore test",
    "clean_debug": "rm -rf .debug && mkdir .debug",
    "build_debug": "babel ./ --out-dir .debug --ignore node_modules --source-maps --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebundala/free-mentors.git"
  },
  "keywords": [
    "free",
    "mentors",
    "social",
    "initiative",
    "profesionals",
    "role",
    "models"
  ],
  "author": "ebundala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ebundala/free-mentors/issues"
  },
  "homepage": "https://github.com/ebundala/free-mentors#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "puppeteer": "^1.19.0",
    "chai-http": "^4.3.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "swagger-ui-express": "^4.0.7",
    "validator": "^11.1.0",
    "yamljs": "^0.3.0"
  }
}
